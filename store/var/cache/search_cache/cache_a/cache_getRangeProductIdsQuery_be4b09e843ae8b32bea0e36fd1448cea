                      3343121569s:1688:"SELECT xcart_products.*, xcart_quick_flags.*, xcart_quick_prices.variantid, xcart_pricing.price, xcart_products_lng_en.product, xcart_products_lng_en.descr, xcart_products_lng_en.fulldescr,  xcart_variants.avail  as avail,  xcart_variants.weight  as weight,  xcart_variants.productcode  as productcode FROM xcart_products INNER JOIN xcart_pricing ON xcart_pricing.productid = xcart_products.productid AND xcart_products.forsale = 'Y' AND xcart_products.product_type <> 'C' INNER JOIN xcart_quick_flags ON xcart_quick_flags.productid = xcart_products.productid INNER JOIN xcart_quick_prices ON xcart_quick_prices.productid = xcart_products.productid AND xcart_quick_prices.membershipid = '0' AND xcart_quick_prices.priceid = xcart_pricing.priceid INNER JOIN xcart_products_lng_en ON xcart_products_lng_en.productid = xcart_products.productid INNER JOIN xcart_products_categories ON  xcart_products_categories.productid = xcart_products.productid AND xcart_products_categories.avail = 'Y'  INNER JOIN xcart_variants ON xcart_variants.productid = xcart_products.productid AND xcart_quick_prices.variantid = xcart_variants.variantid LEFT JOIN xcart_category_memberships ON xcart_category_memberships.categoryid = xcart_products_categories.categoryid LEFT JOIN xcart_product_memberships ON xcart_product_memberships.productid = xcart_products.productid WHERE xcart_products.manufacturerid IN ('7') AND xcart_products_categories.main = 'Y' AND xcart_category_memberships.membershipid IS NULL AND xcart_product_memberships.membershipid IS NULL GROUP BY xcart_products.productid ORDER BY price DESC, xcart_products_lng_en.product ASC, xcart_products.productcode ASC, xcart_products.productid DESC";